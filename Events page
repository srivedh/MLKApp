<script>

$("#loadEvts").bind("click",function(event)
{ 
   var eventList = GetEvents();
   var tableHTML = "<div class=ui-grid-a>";
   for(var i = 0; i<eventList.length; i++)
   {
	   tableHTML+= "<div class='ui-block-a'>"+eventList[i].name+"</div><div class='ui-block-b'>"+eventList[i].description+"</div>";
   }
   tableHTML+="</div>";
   $("#win2 div:jqmData(role=content)").append(tableHTML);
   
$.mobile.changePage($("#win2"));
});
function GetEvents()
{
	var eventList = new Array();
   
   for(var i = 0; i<10; i++)
   {
	   var evt = {name: "Event"+(i+1),
	              description: "Description" +(i+1)};
	   eventList[i] = evt;
   }
   return eventList;
}

</script>
