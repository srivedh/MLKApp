<div data-role=page id=win1>
  <div data-role=header>
    <h1>Window 1</h1>
  </div>

  <div data-role=content>
    <p> Window content 1</p>
    <a id="loadEvts" data-role=button> Get events </a>
    
  </div>
</div>

<div data-role=page id=win2 data-add-back-btn=true>
  <div data-role=header>
    <h1>Events</h1>
  </div>

  <div data-role=content>
    <p> Events</p>
  </div>
</div>

